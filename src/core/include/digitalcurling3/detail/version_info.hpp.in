#include "digitalcurling3/detail/version.hpp"

std::string digitalcurling3::GetVersion()
{
    return "@digitalcurling3_VERSION@";
}
std::uint32_t digitalcurling3::GetVersionMajor()
{
    return @digitalcurling3_VERSION_MAJOR@;
}
std::uint32_t digitalcurling3::GetVersionMinor()
{
    return @digitalcurling3_VERSION_MINOR@;
}
std::uint32_t digitalcurling3::GetVersionPatch()
{
    return @digitalcurling3_VERSION_PATCH@;
}
